name: "CaffeNet"
input: "im_data"
input_shape {
  dim: 1
  dim: 3
  dim: 227
  dim: 227
}
input: "txt_data"
input_shape {
  dim: 1
  dim: 128
  dim: 227
  dim: 227
}
input: "im_rois"
input_shape {
  dim: 1 # to be changed on-the-fly to num ROIs
  dim: 5 # [batch ind, x1, y1, x2, y2] zero-based indexing
}
input: "txt_rois"
input_shape {
  dim: 1 # to be changed on-the-fly to num ROIs
  dim: 5 # [batch ind, x1, y1, x2, y2] zero-based indexing
}
layer{
  name: "txt_ignore"
  type: "Silence"
  bottom: "im_data"
  bottom: "im_rois"
}
layer {
  name: "txt_pool"
  type: "ROIPooling"
  bottom: "txt_data"
  bottom: "txt_rois"
  top: "txt_pool"
  roi_pooling_param {
    pooled_w: 24
    pooled_h: 24
    spatial_scale: 1.0 # 1
  }
}
layer {
  name: "DOM_fc6"
  type: "InnerProduct"
  bottom: "txt_pool"
  top: "DOM_fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "DOM_fc6"
  top: "DOM_fc6"
}
layer {
  name: "drop6"
  type: "Dropout"
  bottom: "DOM_fc6"
  top: "DOM_fc6"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "DOM_fc7"
  type: "InnerProduct"
  bottom: "DOM_fc6"
  top: "DOM_fc7"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu7"
  type: "ReLU"
  bottom: "DOM_fc7"
  top: "DOM_fc7"
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "DOM_fc7"
  top: "DOM_fc7"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "bbox_pred"
  type: "InnerProduct"
  bottom: "DOM_fc7"
  top: "bbox_pred"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 16
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


